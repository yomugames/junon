                <div>
                    <p class='date'><%= changeLog.date %></p>
                    <% if (changeLog.content.bugs.length > 0) { %>
                        <ul>
                            <% changeLog.content.bugs = changeLog.content.bugs.slice(0,1) %>
                            <% for (let i = 0; i < changeLog.content.bugs.length; i++) { %>
                                <% let bug = changeLog.content.bugs[i] %>
                                <li><%= bug %></li>                    
                            <% } %>
                        </ul>
                    <% } %>
                    <% if (changeLog.content.features.length > 0) { %>
                        <ul>
                            <% changeLog.content.features = changeLog.content.features.slice(0,2) %>
                            <% for (let i = 0; i < changeLog.content.features.length; i++) { %>
                                <% let feature = changeLog.content.features[i] %>
                                <li><%= feature %></li>                    
                            <% } %>
                        </ul>
                    <% } %>
                    <a href="/changelogs.html" target="_blank" id='changelog_link'>(See More)</a>
                </div>
            </div>

        </div>
